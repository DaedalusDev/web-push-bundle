<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="web_push.class">Minishlink\WebPush\WebPush</parameter>
    </parameters>

    <services>
        <service id="web_push" class="%web_push.class%">
            <argument type="collection" id="%web_push.api_keys%" />
            <argument type="string" id="%web_push.ttl%" />
            <argument type="string" id="%web_push.timeout%" />
        </service>
    </services>
</container>
